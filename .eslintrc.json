{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "@typescript-prettier"],
  "rules": {
    "quotes": "off",
    "no-console": "off",
    "implicit-arrow-linebreak": "warn"
  }
}
